<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="root" alignment="TOP_CENTER" prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button fx:id="volverButton" mnemonicParsing="false" onAction="#onVolverAction" text="Volver" />
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" top="20.0" />
         </padding>
      </HBox>
      <HBox fx:id="imageHBox" alignment="BASELINE_CENTER" prefHeight="100.0" prefWidth="200.0">
         <children>
            <ImageView fx:id="jugador1ImageView" fitHeight="250.0" fitWidth="250.0" pickOnBounds="true" preserveRatio="true" />
            <ImageView fx:id="jugador2ImageView" fitHeight="250.0" fitWidth="250.0" pickOnBounds="true" preserveRatio="true" />
         </children>
         <padding>
            <Insets bottom="20.0" />
         </padding>
      </HBox>
      <Button fx:id="listoButton" mnemonicParsing="false" text="Listo" />
   </children>
</VBox>
